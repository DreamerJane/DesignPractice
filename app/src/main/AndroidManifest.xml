<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.dycloud.designpractice">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".TextInputLayoutActivity" />
        <activity android:name=".FloatingABActivity" />
        <activity android:name=".SnackBarActivity" />
        <activity android:name=".TabLayoutActivity" />
        <activity android:name=".NaviViewActivity" />
        <activity android:name=".CoordinatorLayoutActivity"></activity>
    </application>

</manifest>
